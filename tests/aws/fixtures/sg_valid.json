{
  "__name": "web-security-group",
  "type": "aws:ec2/securityGroup:SecurityGroup",
  "description": "Allow web traffic only",
  "ingress": [
    {
      "protocol": "tcp",
      "fromPort": 80,
      "toPort": 80,
      "cidrBlocks": ["0.0.0.0/0"]
    },
    {
      "protocol": "tcp",
      "fromPort": 443,
      "toPort": 443,
      "cidrBlocks": ["0.0.0.0/0"]
    },
    {
      "protocol": "tcp",
      "fromPort": 22,
      "toPort": 22,
      "cidrBlocks": ["10.0.0.0/8"]
    }
  ]
}
